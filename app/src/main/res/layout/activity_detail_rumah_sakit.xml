<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroud_pendaftaran"
    tools:context=".view.DetailRumahSakitActivity">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/layout_maps"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        app:layout_constraintTop_toTopOf="parent"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:background="@color/shimmer_placeholder"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        tools:context=".view.DetailRumahSakitActivity" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_list_rs"
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/layout_maps"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:listitem="@layout/item_rumah_sakit_map"
        android:layout_height="0dp"/>





</androidx.constraintlayout.widget.ConstraintLayout>